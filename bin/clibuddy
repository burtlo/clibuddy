#!/usr/bin/env ruby
require "clibuddy/parser/default_parser"

b = CLIBuddy::Prototype::Parser::DefaultParser.new()

begin
  b.run("sample.txt")
  puts "*"*20
  puts "Commands: #{b.commands}"
  puts "*"*20
  puts "Messages: #{b.messages}"
  puts "*"*20
rescue => e
  puts "Exception: #{e.message}"
  puts e.backtrace
  puts "State: "
  puts b.commands
  puts b.messages
end


